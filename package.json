  "devDependencies": {
    "@types/chai": "^3.5.1",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.13",
    "chai": "^3.5.0",
    "chai-json-schema": "^1.4.0",
    "coveralls": "^2.13.1",
    "istanbul": "^0.4.5",
